{
  extends: 'airbnb',
  parser: 'babel-eslint',
  plugins: ['import', 'jsx-a11y','react'],
  env: {
    browser: true,
    node: true,
    es6: true
  },
  rules: {
    'comma-dangle': ['error', 'never'],
    'jsx-quotes': ['error', 'prefer-single'],
    'react/jsx-filename-extension': [1, { "extensions": [".js", ".jsx"] }]
  }
}
